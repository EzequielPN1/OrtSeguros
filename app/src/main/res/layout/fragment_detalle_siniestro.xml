<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout7"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".fragments.home.siniestros.DetalleSiniestroFragment">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Fecha"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toStartOf="@+id/txtFechaSiniestro"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:textSize="30sp"

         />

    <TextView
        android:id="@+id/txtFechaSiniestro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Fecha"
        app:layout_constraintBottom_toTopOf="@+id/txtHoraSiniestro"


        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="parent"

        android:textSize="30sp"/>

    <TextView
        android:id="@+id/txtHoraSiniestro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Hora"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/txtFechaSiniestro"
        app:layout_constraintBottom_toTopOf="@+id/txtUbicacionSiniestro"

        android:textSize="30sp"
        />

    <TextView
        android:id="@+id/txtDescripcionSiniestro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Descripcion"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/txtUbicacionSiniestro"
        app:layout_constraintBottom_toBottomOf="parent"


        android:textSize="30sp"
        />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Ubicacion"

        app:layout_constraintEnd_toStartOf="@+id/txtUbicacionSiniestro"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintBottom_toTopOf="@+id/textView11"

        android:textSize="30sp"
   />

    <TextView
        android:id="@+id/txtUbicacionSiniestro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Ubicacion"


        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/txtHoraSiniestro"
        app:layout_constraintBottom_toTopOf="@+id/txtDescripcionSiniestro"

        android:textSize="30sp"
  />


    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Hora"

        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintEnd_toStartOf="@+id/txtHoraSiniestro"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"

        android:textSize="30sp"
        />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Descripcion"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/txtDescripcionSiniestro"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"

        android:textSize="30sp"
 />

</androidx.constraintlayout.widget.ConstraintLayout>